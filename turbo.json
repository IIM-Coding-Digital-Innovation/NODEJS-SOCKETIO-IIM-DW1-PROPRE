{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".nuxt/**"]
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    },
    "dev": {
      "dependsOn": ["studio"],
      "cache": false
    },
    "studio": {
      "outputs": []
    },
    "migrate": {
      "outputs": ["prisma/migrations/**"]
    }
  }
}
